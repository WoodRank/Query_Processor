{
  "op": "Limit",
  "limit": 5,
  "input": {
    "op": "Project",
    "exprs": [
      { "as": "customer_name", "expr": { "col": "c.name" } },
      { "as": "country", "expr": { "col": "c.country" } },
      { "as": "order_total", "expr": { "col": "o.total" } }
    ],
    "input": {
      "op": "Select",
      "predicate": {
        "op": "GT",
        "left": { "col": "o.total" },
        "right": { "const": 200.0, "type": "float" }
      },
      "input": {
        "op": "Join",
        "condition": {
          "op": "EQ",
          "left": { "col": "o.customer_id" },
          "right": { "col": "c.customer_id" }
        },
        "left": {
          "op": "Select",
          "predicate": {
            "op": "EQ",
            "left": { "col": "o.status" },
            "right": { "const": "OPEN", "type": "string" }
          },
          "input": {
            "op": "Scan",
            "table": "orders.csv",
            "as": "o"
          }
        },
        "right": {
          "op": "Scan",
          "table": "customers.csv",
          "as": "c"
        }
      }
    }
  }
}